
package Negocio.Empleado;

public class TEmpleadoCompleto extends TEmpleado{

	public TEmpleadoCompleto(Integer iD, String nombre, boolean activo, String dni, String telefono, Float sueldo,Integer idDepartamento,Integer HorasSemana) {
		super(iD, nombre, activo, dni, telefono, sueldo, idDepartamento);
		this.HorasSemana=HorasSemana;
	}

	private Integer HorasSemana;

	public Integer getHorasSemana() {
		return HorasSemana;
	}

	public void setHorasSemana(Integer HorasSemana) {
		this.HorasSemana = HorasSemana;
	}



}